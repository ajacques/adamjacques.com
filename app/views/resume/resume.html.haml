%aside
  - @resume.key_points.each do |group|
    %span.group
      .title= group.value
      %ul
        - group.sub_points.each do |point|
          %li= point.value
#contents
  .credentialGroup
    .title= t 'resume_section.education'
    = render partial: 'edu_credential', collection: @resume.education, as: :edu
  .credentialGroup
    .title= t 'resume_section.jobs'
    = render partial: 'job_credential', collection: @resume.jobs, as: :job
